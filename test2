setTimeout(() => {
    console.log(1);

    Promise.resolve().then(()=>{
        console.log(2);
    });
    process.nextTick(() => {
        console.log(7)
    })
    console.log(3);
}, 0);

setTimeout(() => {
    console.log(4);

    Promise.resolve().then(()=>{
        console.log(5);
    });
    process.nextTick(() => {
        console.log(8)
    })
    console.log(6);
}, 0);
